let dna3
function DNAStrand(dna){
  console.log(dna)
  let dna2 = dna.split('')
  console.log(dna2)
  for (i = 0; i < dna2.length; i++) {
    if (dna2[i] === 'A') {
      dna2[i] = 'T';
    } else  if (dna2[i] === 'T') {
      dna2[i] = 'A';
    } else  if (dna2[i] === 'G') {
      dna2[i] = 'C';
    } else  if (dna2[i] === 'C') {
      dna2[i] = 'G';
    }
  }
    
    
    
  
  console.log(dna2)
  let dna3 = dna2.join('')
  return dna3
}
