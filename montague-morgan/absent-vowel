function absentVowel(x){
  x = x.toLowerCase(x)
  console.log(x)
  let vowel = ['a', 'e', 'i', 'o', 'u' ]
  for( let i of vowel){
    if(!x.includes(i)){
     return vowel.indexOf(i)
    }
  }
}

