function calculate(str) {
str = str.split('plus')
  console.log(str)
  str = str.join('+')
  console.log(str)
  str = str.split('minus')
  str = str.join('-')
  let answer = eval(str)
  console.log(answer)
  answer = answer.toString()
return answer;
}

