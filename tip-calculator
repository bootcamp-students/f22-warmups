function calculateTip(amount, rating) {
  console.log(amount)
  console.log(rating)
   rating = rating.toUpperCase()
  amount = Math.ceil(amount)
  console.log(rating)
  let tip = 0;
   if (rating === 'TERRIBLE') {
     let tip = 0
     return tip
   }
  else if (rating === 'POOR') {
    let tip =  Math.ceil(amount * .05)
    return tip
  }
  else if (rating === 'GOOD') {
    let tip = Math.round(amount * .10)
    return tip
  }
    else if (rating === 'GREAT') {
    let tip = Math.ceil(amount * .15)
    return tip
  }
    else if (rating === 'EXCELLENT') {
    let tip = Math.round(amount * .20)
    console.log(tip)
    return tip
  }
  else {
    tip = 'Rating not recognised'
    return tip
  }
   
}
