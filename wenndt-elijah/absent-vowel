//first check if the string does NOT include a letter
//this is an inefficient way, but next you check every letter too see if it is in the string

function absentVowel(x){
  if(!x.includes('a')){
    return 0
  }
  else if (!x.includes('e')){
    return 1
  }
  else if(!x.includes('i')){
    return 2
  }
  else if (!x.includes('o')){
    return 3
  }
  else if (!x.includes('u')){
    return 4
  }
    
  }
