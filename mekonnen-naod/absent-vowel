// find out which index has a missisng vowel
// check to see if any of the indices are missing any vowels

function absentVowel(x){

  let vowels = ['a','e','i','o','u'];
  
  if (!x.includes('a')){
  return 0}
  else if(!x.includes('e')){
  return 1}
  else if(!x.includes('i')){
  return 2}
  else if(!x.includes('o')){
  return 3}
  else if(!x.includes('u')){
  return 4}
}
