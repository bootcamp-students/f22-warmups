function createPhoneNumber(numbers){
  let var1 = numbers;
      //let varX = numbers[i];
      var1.splice(0, 0, "(");
      var1.splice(4, 0, ")");
      var1.splice(5, 0, " ");
      var1.splice(9, 0, '-');
      //var1 = var1.toString();
  
  let answer = '';
 
  for (let i = 0; i < var1.length; i++){
    //console.log(var1[i]);
    answer = answer + var1[i];
  
  }
  console.log(answer);
  console.log(typeof answer);
  return answer;
}
